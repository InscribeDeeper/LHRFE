									<a href="/furniture/{{this._id}}"><span class="bold">&starf;</span>{{this.name}}</a>
									<div>likes: {{this.likes}}</div>
									<div>num_comments: {{this.num_comments}}</div>
								</li>
							{{/each}}
						</ul>
					{{/if}}
				</div>
			</div>
		</div>
	</div>

	<div class="row mb-5">
		<div class="col card shadow">
			<div class="card-body">
				<div class="container">
					<h2 class="card-title h3">My Reviews History</h2>
				</div>
				<div class="container">
					<ul class="list-group list-group-flush">

						{{#each reviewHistory}}
							<li class="list-group-item borderless">
								<div class="card container mb-5">
									<div class="card-header row">
										<div class="col mt-2">
											<h2 class="h5">{{this.type}}: {{this.name}}</h2>
										</div>
										<div class="btnContainer col">
											<button
												class="btn btn-danger"
												type="button"
												data-toggle="modal"
												data-target="#review-{{@index}}"
												aria-hidden="true"
											>Delete</button>
										</div>
										<div class="modal" id="review-{{@index}}" role="dialog" tabindex="-1">
											<div class="modal-dialog">
												<div class="modal-content">
													<div class="modal-header">
														<h3 class="modal-title text-truncate">{{this.text}}"</h3>
														<button type="button" class="close" data-dismiss="modal" aria-label="Close">
															<span aria-hidden="true">&times;</span>
														</button>
													</div>
													<div class="modal-body">Are you sure you want to delete this review for <span class="bold">{{this.name}}</span>?</div>
													<div class="modal-footer">
														<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
														<button
															type="button"
															class="btn btn-danger delbtn"
															data-id="{{this._id}}"
															data-type="{{this.type}}"
															data-comment_id="{{this.comment_id}}"
															data-dismiss="modal"
														>Delete</button>
													</div>
												</div>
											</div>
										</div>
									</div>

